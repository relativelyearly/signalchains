- semantic_form_for @effects_processor, :html => {:multipart => true} do |f|
  = render(:partial => 'gear/form', :locals => {:f => f})

  = f.input :effects_type, :as => :select, :collection => EffectsProcessor.effects_types, :label => 'Type'
  = f.input :interface, :as => :select, :collection => EffectsProcessor.interfaces
  - if @chain
    = hidden_field_tag :chain_id, @chain.id
  = f.buttons do
    = f.commit_button 'Create Effects Processor'