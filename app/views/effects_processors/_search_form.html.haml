- semantic_form_for @search, :url => {:controller => :effects_processors, :action => :search}, :html => {:id => 'search_form'} do |f|
  = f.input :make_like, :input_html => { :size => 27 }
  = f.input :model_like, :input_html => { :size => 27 }

  = f.input :effects_type, :as => :select, :collection => EffectsProcessor.effects_types, :label => 'Type'
  = f.input :interface, :as => :select, :collection => EffectsProcessor.interfaces