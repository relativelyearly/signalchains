- title "Effects Processors"

#search
  = render(:partial => 'effects_processors/search_form')

%br.clear

#gear= render(:partial => 'effects_processors/effects_processor', :collection => @effects_processors)

.w15
  = link_to "<span>New Processor</span>", @chain.nil? ? new_effects_processor_path : new_chain_effects_processor_path(@chain), :class => 'btn'


:javascript
  $(function () {
    $('input').keyup(search);
    $('select').change(search);
  });

  function search() {
    $('#search_form').ajaxSubmit({ target: '#gear' });
  }